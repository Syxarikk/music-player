# YouTube API Key (get from Google Cloud Console)
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here

# YouTube Server URL (optional, for server mode)
VITE_YOUTUBE_SERVER_URL=http://your-server-ip:3000

# Server-side YouTube API Key (for Electron main process)
YOUTUBE_API_KEY=your_youtube_api_key_here

